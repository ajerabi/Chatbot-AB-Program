<category><pattern>CHECKCOMPUTER *</pattern>
<template><think>
            <set name="PROCESSOR">DEFAULT</set>
            <set name="HARDDRIVE">DEFAULT</set>
            <set name="MOTHERBOARD">DEFAULT</set>
            <set name="RAM">DEFAULT</set>
            <set name="COOLER">DEFAULT</set>
            <set name="VIDEOCARD">DEFAULT</set>
            <set name="SOUNDCARD">DEFAULT</set>
            <set name="CHASSIS">DEFAULT</set>
        </think></template>
</category>
<category><pattern>COMPUTER SPECIFICATIONS</pattern>
<template>INI COMPUTER MU
            <get name="PROCESSOR_LV"/> and <get name="PROCESSOR_LN"/>
            <get name="HARDDRIVE_LV"/>
            <get name="MOTHERBOARD_LV"/>
            <get name="RAM_LV"/>
            <get name="COOLER_LV"/>
            <get name="VIDEOCARD_LV"/>
            <get name="SOUNDCARD_LV"/></template>
</category>
</aiml>
